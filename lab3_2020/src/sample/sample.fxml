<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
            prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sample.Controller">
    <children>
        <TextArea fx:id="textArea" layoutX="278.0" layoutY="35.0" prefHeight="310.0" prefWidth="286.0"/>
        <Button fx:id="buttonAdd" disable="true" layoutX="50.0" layoutY="85.0" mnemonicParsing="false"
                onAction="#handleButtonAdd" prefHeight="29.0" prefWidth="161.0" text="Добавить новый объект"/>
        <Button fx:id="buttonDelete" disable="true" layoutX="50.0" layoutY="135.0" mnemonicParsing="false"
                onAction="#handleButtonDelete" prefHeight="29.0" prefWidth="161.0" text="Удалить объект из списка"/>
        <Button fx:id="buttonPrint" disable="true" layoutX="50.0" layoutY="185.0" mnemonicParsing="false"
                onAction="#handleButtonPrint" prefHeight="29.0" prefWidth="161.0" text="Вывод списка семьи"/>
        <Button fx:id="buttonExit" layoutX="50.0" layoutY="235.0" mnemonicParsing="false" onAction="#handleButtonExit"
                prefHeight="29.0" prefWidth="161.0" text="Выход"/>
        <Button fx:id="buttonStart" layoutX="50.0" layoutY="35.0" mnemonicParsing="false" onAction="#handleButtonStart"
                prefHeight="29.0" prefWidth="161.0" text="Начать"/>
    </children>
</AnchorPane>
